{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "REST service starter for RS School",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "ts-node src/server.ts",
    "lint": "eslint --ext .ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "prettier": "prettier src/ --write",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/common/ormconfig-migration.ts",
    "typeorm:sh": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/common/ormconfig.ts",
    "migration": "npm run typeorm migration:run",
    "migration:generation": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:sh": "npm run typeorm:sh migration:run",
    "migration:sh:generation": "npm run typeorm:sh migration:generate -- -n",
    "migration:sh:revert": "npm run typeorm:sh migration:revert"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0 <15"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "clean-jsdoc-theme": "^3.2.7",
    "cross-env": "7.0.3",
    "dotenv": "9.0.0",
    "express": "4.17.1",
    "express-async-handler": "^1.1.4",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.0",
    "pg": "^8.6.0",
    "prettier-plugin-jsdoc": "^0.3.22",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/hapi__joi": "^17.1.6",
    "@types/joi": "^17.2.3",
    "@types/node": "^15.6.1",
    "@types/request": "^2.48.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "4.3.4",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jsdoc": "~3.6.7",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
